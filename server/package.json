  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest",
    "test:unit": "NODE_ENV=test jest --'.*\\.(spec)\\.ts$'",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --testRegex '.*\\.(e2e-spec)\\.ts$'",
    "migrate": "npx sequelize db:migrate",
    "migrate:u": "npx sequelize db:migrate:undo",
    "migrate:u:all": "npx sequelize db:migrate:undo:all",
    "migration:gen": "npx sequelize migration:generate",
    "seed": "npx sequelize db:seed:all",
    "stripe:listen": "stripe listen --forward-to https://localhost:2000/webhook/stripe --skip-verify",
    "init:submodules": "bash scripts/init-submodules.sh",
    "update:emails": "bash scripts/update-emails.sh",
    "deploy": "bash ./scripts/deploy.sh --install --pull --update-emails --migrate-db",
    "logs": "bash ./scripts/pm2-logs.sh"
  },
